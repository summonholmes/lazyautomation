// Place your key bindings in this file to override the defaultsauto[]
[
    {
        "key": "shift+cmd+f",
        "command": "editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "shift+alt+f",
        "command": "-editor.action.formatDocument",
        "when": "editorHasDocumentFormattingProvider && editorHasDocumentFormattingProvider && editorTextFocus && !editorReadonly && !inCompositeEditor"
    },
    {
        "key": "shift+cmd+backspace",
        "command": "notebook.clearAllCellsOutputs"
    },
    {
        "key": "alt+delete",
        "command": "-notebook.cell.clearOutputs",
        "when": "notebookCellHasOutputs && notebookEditorFocused && !inputFocus"
    },
    {
        "key": "shift+cmd+backspace",
        "command": "notebook.cell.clearOutputs"
    },
    {
        "key": "shift+cmd+backspace",
        "command": "jupyter.removeallcells"
    },
    {
        "key": "shift+cmd+m",
        "command": "jupyter.importnotebook"
    },
    {
        "key": "shift+cmd+r",
        "command": "jupyter.restartkernel"
    },
    {
        "key": "shift+cmd+i",
        "command": "jupyter.interruptkernel"
    },
    {
        "key": "shift+enter",
        "command": "-jupyter.execSelectionInteractive",
        "when": "editorTextFocus && jupyter.ownsSelection && !findInputFocussed && !notebookEditorFocused && !replaceInputFocussed && editorLangId == 'python'"
    },
    {
        "key": "shift+enter",
        "command": "-python.execSelectionInTerminal",
        "when": "editorTextFocus && !findInputFocussed && !jupyter.ownsSelection && !replaceInputFocussed && editorLangId == 'python'"
    }
]